  <transform>
    <name>Añadir campo Distrito según las coordenadas</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

// Definición de centroides aproximados de los distritos de Madrid
// Formato: {codigo, latitud, longitud}
var districts = [
    { code: 2807901, lat: 40.4155, lon: -3.7074 }, // Centro
    { code: 2807902, lat: 40.3980, lon: -3.6937 }, // Arganzuela
    { code: 2807903, lat: 40.4111, lon: -3.6766 }, // Retiro
    { code: 2807904, lat: 40.4273, lon: -3.6775 }, // Salamanca
    { code: 2807905, lat: 40.4526, lon: -3.6779 }, // Chamartín
    { code: 2807906, lat: 40.4605, lon: -3.6983 }, // Tetuán
    { code: 2807907, lat: 40.4362, lon: -3.7038 }, // Chamberí
    { code: 2807908, lat: 40.4900, lon: -3.7100 }, // Fuencarral-El Pardo
    { code: 2807909, lat: 40.4397, lon: -3.7300 }, // Moncloa-Aravaca
    { code: 2807910, lat: 40.4036, lon: -3.7430 }, // Latina
    { code: 2807911, lat: 40.3800, lon: -3.7400 }, // Carabanchel
    { code: 2807912, lat: 40.3820, lon: -3.7050 }, // Usera
    { code: 2807913, lat: 40.3890, lon: -3.6600 }, // Puente de Vallecas
    { code: 2807914, lat: 40.4050, lon: -3.6450 }, // Moratalaz
    { code: 2807915, lat: 40.4450, lon: -3.6500 }, // Ciudad Lineal
    { code: 2807916, lat: 40.4700, lon: -3.6400 }, // Hortaleza
    { code: 2807917, lat: 40.3450, lon: -3.6950 }, // Villaverde
    { code: 2807918, lat: 40.3700, lon: -3.6100 }, // Villa de Vallecas
    { code: 2807919, lat: 40.4000, lon: -3.6000 }, // Vicálvaro
    { code: 2807920, lat: 40.4300, lon: -3.6200 }, // San Blas-Canillejas
    { code: 2807921, lat: 40.4700, lon: -3.5800 }  // Barajas
];

var input_lat = stop_lat;
var input_lon = stop_lon; 

var min_dist = 99999999;
var distrito_asignado = 0; 

// Si las coordenadas son válidas
if (input_lat != null &amp;&amp; input_lon != null) {
    for (var i = 0; i &lt; districts.length; i++) {
        var d = districts[i];
        
        // Cálculo de distancia euclidiana simple (suficiente para asignar cercanía)
        var dist = (input_lat - d.lat) * (input_lat - d.lat) + 
                   (input_lon - d.lon) * (input_lon - d.lon);
        
        if (dist &lt; min_dist) {
            min_dist = dist;
            distrito_asignado = d.code;
        }
    }
}</jsScript_script>

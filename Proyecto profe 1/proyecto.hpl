        <jsScript_script>//Script here

// 1. Mapa de distritos corregido
var distrito_map = {
    "01": "Centro", "02": "Arganzuela", "03": "Retiro", "04": "Salamanca",
    "05": "Chamartin", "06": "Tetuan", "07": "Chamberi", "08": "Fuencarral-El Pardo",
    "09": "Moncloa-Aravaca", "10": "Latina", "11": "Carabanchel", "12": "Usera",
    "13": "Puente de Vallecas", "14": "Moratalaz", "15": "Ciudad Lineal", "16": "Hortaleza",
    "17": "Villaverde", "18": "Villa de Vallecas", "19": "Vicalvaro", "20": "San Blas",
    "21": "Barajas"
};

// 2. Obtener el código limpio de "zona_pernoctacion" (ej. "2807901")
var zona_str = "";
var nuevo_nombre = name; // Por defecto dejamos el que ya existía por seguridad

if (zona_pernoctacion != null) {
    zona_str = zona_pernoctacion.toString().trim();
    
    // Cogemos siempre los últimos 2 caracteres con slice(-2)
    // Esto convierte "2807901" en "01" y "2807921" en "21"
    var codigo_limpio = zona_str.slice(-2);
    
    // 3. Buscamos en el mapa
    if (distrito_map[codigo_limpio] != undefined) {
        nuevo_nombre = distrito_map[codigo_limpio];
    }
}</jsScript_script>
